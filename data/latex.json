            "\\newtheorem{note}                {Note}",
            "\\newtheorem{claim}               {Claim}",
            "\\newtheorem{summary}             {Summary}",
            "\\newtheorem{case}                {Case}",
            "\\newtheorem{acknowledgment}      {Acknowledgments}",
            "\\newtheorem{conclusion}          {Conclusion}",
            "\\newtheorem{notation}            {Notation}",
            "\t\t\\numberwithin{equation}{section}",
            "%----------------------------------------------------",
            "\\begin{document}",
            "\\title[put short title here]%",
            "\t  {Put the real title here\\\\",
            "\t\tand rest of title here}",
            "\\author[Author One and Author Two]{%",
            "\t\tAuthor One\\\\",
            "\t\tAuthor Two\\\\",
            "}",
            "\t\t\\address{Department of Mathematics\\\\",
            "\t\t\t\t Northeastern University\\\\",
            "\t\t\t\t Boston, Massachusetts 02115}%",
            "\t\t\\address{Mathematical Research Section\\\\",
            "\t\t\t\t School of Mathematical Sciences\\\\",
            "\t\t\t\t Australian National University\\\\",
            "\t\t\t\t Canberra ACT 2601, Australia}",
            "\\subjclass{Primary 54C40, 14E20; Secondary 46E25, 20C20}",
            "\\date     {July 2, 1991}",
            "%         \\thanks will become a 1st page footnote.",
            "\\thanks{The first author was supported in part by NSF",
            "\t\tGrant \\#000000.}",
            "%\\dedicatory{}",
            "\\maketitle",
            "\\begin{abstract}",
            "\t\tThis is the abstract.",
            "\\end{abstract}",
            "\\tableofcontents",
            "\\section{Other headings}",
            "\\section*{This is an unnumbered first-level section head}",
            "\\subsection{This is a numbered second-level section head}",
            "\\subsection*{This is an unnumbered second-level section head}",
            "\\subsubsection{This is a numbered third-level section head}",
            "\\subsubsection*{This is an unnumbered third-level section head}",
            "\\begin{theorem}",
            "\t\tDefinition environemnt",
            "\\end{theorem}",
            "\\begin{definition}",
            "\t\tDefinition environemnt",
            "\\end{definition}",
            "\\begin{remark}",
            "\t\tRemark environemnt",
            "\\end{remark}",
            "%-----------------",
            "\\bibliographystyle{}",
            "\\begin{thebibliography}{}",
            "\\bibitem[{Arnol\\$'\\$d et al. [1982]}] {arnold:sing}",
            "\t\t Arnol\\$'\\$d, V. L., Varchenko, A. N.,",
            "\t\t  and Gusein-Zade, S. M. [1982]",
            "\t\t \\emph{Singularities of differentiable maps.} I,",
            "\t\t  ``Nauka'', Moscow (Russian);",
            "\t\t English transl. Birkh\\\"auser, 1985.",
            "\\bibitem[{Arnol\\$'\\$d et al. [1984]}] {arnold:sing2}",
            "\\bysame [1984]",
            "\t\t\\emph{Singularities of differentiable maps.}~II,",
